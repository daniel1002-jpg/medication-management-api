    "pg": "8.16.3"
  },
  "devDependencies": {
    "baseline-browser-mapping": "^2.9.9",
    "dotenv-cli": "^11.0.0",
    "jest": "30.2.0",
    "node-pg-migrate": "^8.0.4",
    "nodemon": "^3.1.13",
    "supertest": "7.1.4"
  },
  "engines": {
    "node": ">=18"
  }
