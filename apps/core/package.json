  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:unit:entities": "jest tests/unit/domain/entities",
    "test:unit:cases": "jest tests/unit/application/use-cases",
    "test:unit:repositories": "jest tests/unit/infrastructure/database",
    "test:unit:controllers": "jest tests/unit/interfaces/controllers",
    "test:integration": "node --experimental-vm-modules node_modules/.bin/jest tests/integration",
    "migrate": "dotenv -e ../../.env -- node-pg-migrate up -m migrations",
"migrate:test": "dotenv -e ../../.env -- node-pg-migrate up -d $DATABASE_TEST_URL -m migrations"
  },
